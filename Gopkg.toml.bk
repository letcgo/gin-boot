# 必需包
required = [
    "github.com/letcgo/gin",
    "github.com/go-yaml/yaml"
    ]
# 忽略包
ignored = ["golang.org/x/crypto"]
# 项目元数据
[metadata]
homepage = "https://github.com/letcgo"
license = "MIT"

# 约束条件
[[constraint]]
  name = "github.com/letcgo/gin"
  # 可选：版本
  version = "^1.3.0"
  # 分支
  #branch = "master"
  # 修订
  #revision = "beego 1.8.0"
  # 可选：指定来源
  source = "github.com/astaxie/beego"